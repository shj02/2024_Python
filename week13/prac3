cars = []

while True:
    number = input("차량번호: ").strip()
    if not number:
        break

    intime = input("입차시간: ")
    outtime = input("출차시간: ")

    car = {'num':number, 'in':intime, 'out':outtime}
    cars.append(car)

for car in cars:
    print(car['num'], car['in'], car['out'])
